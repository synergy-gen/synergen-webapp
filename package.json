{
  "name": "synergen-webclient",
  "version": "1.0.0",
  "description": "Simple template for bootstraping development environment for ReactJS app",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node node_modules/.bin/webpack-dev-server --mode development --open --hot",
